# Root Project .gitignore

# OS generated files
.DS_Store
.DS_Store?
._*
.Spotlight-V100
.Trashes
ehthumbs.db
Thumbs.db
VolumeIcon.icns

# Log files
logs/
*.log
.qwen
.claude


# Environment variables
.env
.env.local
.env.dev
.env.test
.env.prod
.env.development.local
.env.test.local
.env.production.local

# Secrets and sensitive data
secrets/
*.key
*.pem
*.crt
*.cert

# Build outputs
dist/
build/
target/
.cache/

# Coverage directory used by tools like istanbul
coverage/
